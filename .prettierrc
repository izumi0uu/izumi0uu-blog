{
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-astro"],
  "tailwindFunctions": ["clsx", "cn", "cva"],
  "tailwindAttributes": ["class", "className"],
  "tailwindStylesheet": "./src/styles/main.css",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
